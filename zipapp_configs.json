{
  "source": "../zipapp-creator",
  "entry": "main.py",
  "target": "{SOURCE}.pyzw",
  "shebang": "/usr/bin/env python3",
  "compressed": true,
  "self_extract": true,
  "exclude_from_copy": [
    "venv",
    ".venv",
    ".git",
    ".svn",
    ".hg",
    ".vscode",
    ".idea",
    "build",
    "dist"
  ],
  "exclude_from_packaging": [
    "venv",
    ".venv",
    ".git",
    ".svn",
    ".hg",
    ".vscode",
    ".idea",
    "build/",
    "dist/",
    "poetry.lock",
    "pyproject.toml",
    "requirements.txt",
    "*.egg-info",
    "uv.lock",
    "pyproject.toml",
    "*.spec",
    "*.bat",
    ".gitignore",
    "zipapp_configs.json",
    "tests/*",
    "screenshoot/*"
  ],
  "host_py": "python",
  "requirements": "",
  "pip_index_url": "",
  "cleanup_dependencies": true
}